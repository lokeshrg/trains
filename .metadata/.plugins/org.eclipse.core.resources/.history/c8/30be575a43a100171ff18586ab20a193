package Calculator;

public class Addition {

	public static void main(String[] args) {
		int result;
		result = Integer.valueOf(args[0]) + Integer.valueOf(args[1]);
		
		// TODO Auto-generated method stub
		System.out.println("The addition of two numbers is " + args[0] + " : " + args[1]);
		int r2 = addTwo(2, 3);

	}

	private static int addTwo(int i, int j) {
		// TODO Auto-generated method stub
		return i+j;
	}

}
